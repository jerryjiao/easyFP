    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.4.1",
    "standard-version": "^6.0.1",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
